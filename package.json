{
  "name": "comments-microapi-sdk",
  "version": "2.0.11",
  "description": "This is an SDK for the Comments Microapi at Microapi.dev Organization ",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --verbose",
    "test:debug": "cross-env NODE_ENV=test jest --detectOpenHandles --verbose",
    "test:watch": "cross-env NODE_ENV=test jest --watch --verbose",
    "test:cover": "cross-env NODE_ENV=test jest --coverage",
    "test:ci": "cross-env NODE_ENV=test jest --coverage --verbose && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint \"./**/*.js\"",
    "lint:fix": "eslint --fix \"./**/*.js\""
  },
  "keywords": [
    "sdk",
    "comments",
    "replies",
    "microapi",
    "microservice",
    "organizations",
    "applications",
    "tokens",
    "utility",
    "services"
  ],
  "author": "Oscar Ekeyekwu (oscarekeyekwu@gmail.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microapidev/comments-microapi-sdk.git"
  },
  "homepage": "https://github.com/microapidev/comments-microapi-sdk",
  "bugs": {
    "url": "https://github.com/microapidev/comments-microapi-sdk/issues"
  },
  "private": false,
  "dependencies": {
    "dotenv": "^8.2.0",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "request-promise": "^4.2.5"
  },
  "devDependencies": {
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@shelf/jest-mongodb": "^1.2.1",
    "babel-jest": "^26.1.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "mongoose": "^5.9.24",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  }
}
